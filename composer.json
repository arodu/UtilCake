{
    "name": "arodu/utilcake",
    "description": "UtilCake plugin for CakePHP 4.x",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "UtilCake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UtilCake\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
